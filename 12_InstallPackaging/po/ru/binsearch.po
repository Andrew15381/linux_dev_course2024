#: binsearch.c:83
#, c-format
msgid ""
"Number guessing game\n"
"Usage: binsearch [-r | --help]\n"
"Options:\n"
"-r - use roman integers (uppercase)\n"
"--help - print this message\n"
msgstr "Игра по угадыванию чисел\n"
"Использование: binsearch [-r | --help]\n"
"Опции:\n"
"-r - использовать римские цифры (в верхнем регистре)\n"
"--help - вывести это сообщение\n"

#: binsearch.c:90
#, c-format
msgid "Failed parsing arguments, unknown argument: %s\n"
msgstr "Невозможно разобрать агрументы, неизвестный аргумент: %s\n"

#: binsearch.c:99
#, c-format
msgid "Pick a number between %s and %s\n"
msgstr "Загадай число от %s до %s\n"

#: binsearch.c:102
#, c-format
msgid "Is it greater than %s (y/n)?\n"
msgstr "Оно больше чем %s (д/н)?\n"

#: binsearch.c:109
msgid "y"
msgstr "д"

#: binsearch.c:112
msgid "n"
msgstr "н"

#: binsearch.c:116
#, c-format
msgid "Invalid answer, use (y/n)\n"
msgstr "Неверный ответ, используй (д/н)\n"

#: binsearch.c:123
#, c-format
msgid "Failed reading answer\n"
msgstr "Невозможно прочесть\n"

#: binsearch.c:130
#, c-format
msgid "You picked %s\n"
msgstr "Ты загадал %s\n"

